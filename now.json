{
  "name": "thecodex",
  "version": 2,
  "builds": [
    {
      "src": "static/*",
      "use": "@now/static"
    },
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "^/favicon.ico$",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/about"
    },
    {
      "src": "/([^/]+)",
      "dest": "/index?slug=$1"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
